    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  },
  "lint-staged": {
    "{}/**/*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn prettier:fix"
    ]
  }
