{
  "name": "react-date-painter",
  "version": "0.0.0",
  "repository": "git@github.com:Yuta07/react-date-painter.git",
  "author": "yutaka.miyazaki <mono.12dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint '{react-date-painter, example}/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix '{react-date-painter, example}/**/*.{js,jsx,ts,tsx}'",
    "prettier": "prettier '{react-date-painter, example}/**/*.{js,jsx,ts,tsx}'",
    "prettier:fix": "prettier --write '{react-date-painter, example}/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.15.4",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.25.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.2"
  },
  "lint-staged": {
    "{react-date-painter, example}/**/*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn prettier:fix"
    ]
  },
  "dependencies": {
    "dayjs": "^1.10.6"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
